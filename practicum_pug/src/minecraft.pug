extends template

block nav
  div.nav
    h4.nav__title= title
    div.nav__menu
      +createMenu(dictMenu, 'Minecraft')

block content
  div.content
    div.content__title
      h2 Minecraft
    div.content__images
      each image in minecraftImages
        img(src= require("./images/" + image))
    div.content__information
      +createInformation(minecraftInfo)